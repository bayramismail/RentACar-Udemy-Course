{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "RentACar": "Server=(localdb)\\mssqllocaldb; Database=RentACarCourseDb; Trusted_Connection=true"
  },
  "CacheSettings": {
    "SlidingExpiration": 1
  },
  "SerilLogConfigurations": {
    "FileLogConfiguration": {
      "FolderPath": "/logs/"
    },
    "MssqlConfiguration": {
      "ConnectionString": "Server=(localdb)\\mssqllocaldb; Database=RentACarCourseDb; Trusted_Connection=true",
      "TableName": "Logs",
      "AutoCreateSqlTable": true
    }
  },
  "AllowedHosts": "*"
}
